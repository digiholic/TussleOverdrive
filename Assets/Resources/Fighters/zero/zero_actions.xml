		</changeFighterSpeed>
	</conditional>
	
	<conditional name="elecDash">
		<changeSprite>sideSpecialElec</changeSprite>
		<playSound>elec</playSound>
	</conditional>
	
	<conditional name="activateNormal">
		<activateHitbox>dashBox</activateHitbox>
	</conditional>
	
	<conditional name="activateElec">
		<activateArticle>elecDash</activateArticle>
		<activateHitbox>elecChain</activateHitbox>
	</conditional>
	
	<conditional name="elecHitbox">
		<modifyHitbox name="dashBox">
			<hitlag_multiplier>4</hitlag_multiplier>
			<damage>4</damage>
			<base_knockback>8</base_knockback>
		</modifyHitbox>
		<activateHitbox>dashBox</activateHitbox>
	</conditional>
</ForwardSpecial>

<DownSpecial>
	<length>0</length>
	
	<setUp>
		<if>
			<variable source="fighter">element</variable>
			<value>Fire</value>
			<pass>fireToIce</pass>
			<fail>notFire</fail>
		</if>
		<loadArticle name="elem">ElementIcon</loadArticle>
		<activateArticle>elem</activateArticle>
	</setUp>
	
	<conditional name="notFire">
		<if>
			<variable source="fighter">element</variable>
			<value>Ice</value>
			<pass>iceToElec</pass>
			<fail>elecToFire</fail>
		</if>
	</conditional>
	
	<conditional name="fireToIce">
		<setFighterVar var="element">
			<value>Ice</value>
		</setFighterVar>
		<print>Ice</print>
	</conditional>
	
	<conditional name="iceToElec">
		<setFighterVar var="element">
			<value>Elec</value>
		</setFighterVar>
		<print>Elec</print>
	</conditional>
	
	<conditional name="elecToFire">
		<setFighterVar var="element">
			<value>Fire</value>
		</setFighterVar>
		<print>Fire</print>
	</conditional>
</DownSpecial>

</actionList>